{
  "name": "@vn/core",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./ports": {
      "types": "./dist/src/ports.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./commands": {
      "types": "./dist/src/commands/registry.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "build": "npm run types && tsup src/index.ts --format esm,cjs --out-dir dist",
    "prepublishOnly": "npm run build && npm run test"
  },
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "vitest": "^4.0.4"
  }
}

{
  "name": "@vn/script",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "types": "tsc -b",
    "build": "npm run types && tsup index.ts --format esm,cjs --out-dir dist",
    "prepublishOnly": "npm run build && npm run test"
  },
  "license": "MIT",
  "publishConfig": { "access": "public" },
  "dependencies": {},
  "devDependencies": {}
}
